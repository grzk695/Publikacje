<%= bootstrap_form_for(@publication) do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :description %>
  <%= f.text_field :publication_subtype_id %>
  <%= f.submit "Create Publication" %>
  <%= select_tag(:author_id,options_for_select(@authors),multiple: true) %>
<% end %>
